<%- include("partials/header")%>
<h1>Compose</h1>

<form action="" method="post">
  <div class="form-group">
    <label>Title</label>

    <input type="text" name="postTitle" class="form-control" autocomplete="off"/>
    <label>Post</label>
    <textarea
      name="postBody"
      class="form-control"
      cols="30"
      rows="5"
    ></textarea>
  </div>
  <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer")%>
